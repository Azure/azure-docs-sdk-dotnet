    <Member MemberName="AuditActionsAndGroups">
      <MemberSignature Language="C#" Value="public Azure.Provisioning.BicepList&lt;string&gt; AuditActionsAndGroups { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance class Azure.Provisioning.BicepList`1&lt;string&gt; AuditActionsAndGroups" />
      <MemberSignature Language="DocId" Value="P:Azure.Provisioning.Sql.SqlServerBlobAuditingPolicy.AuditActionsAndGroups" />
