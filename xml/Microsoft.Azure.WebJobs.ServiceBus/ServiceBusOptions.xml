    <Member MemberName="MaxBatchWaitTime">
      <MemberSignature Language="C#" Value="public TimeSpan MaxBatchWaitTime { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.TimeSpan MaxBatchWaitTime" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Azure.WebJobs.ServiceBus.ServiceBusOptions.MaxBatchWaitTime" />
      <MemberSignature Language="VB.NET" Value="Public Property MaxBatchWaitTime As TimeSpan" />
      <MemberSignature Language="F#" Value="member this.MaxBatchWaitTime : TimeSpan with get, set" Usage="Microsoft.Azure.WebJobs.ServiceBus.ServiceBusOptions.MaxBatchWaitTime" />
      <MemberType>Property</MemberType>
