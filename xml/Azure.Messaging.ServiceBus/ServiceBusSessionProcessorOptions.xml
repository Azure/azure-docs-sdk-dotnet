      <MemberSignature Language="C#" Value="public TimeSpan? SessionIdleTimeout { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.Nullable`1&lt;valuetype System.TimeSpan&gt; SessionIdleTimeout" />
