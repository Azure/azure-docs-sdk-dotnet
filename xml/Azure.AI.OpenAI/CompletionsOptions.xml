    <Member MemberName="GenerationSampleCount">
      <MemberSignature Language="C#" Value="public int? GenerationSampleCount { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.Nullable`1&lt;int32&gt; GenerationSampleCount" />
      <MemberSignature Language="DocId" Value="P:Azure.AI.OpenAI.CompletionsOptions.GenerationSampleCount" />
