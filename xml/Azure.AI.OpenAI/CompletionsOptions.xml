      <MemberSignature Language="F#" Value="member this.NucleusSamplingFactor : Nullable&lt;single&gt; with get, set" Usage="Azure.AI.OpenAI.CompletionsOptions.NucleusSamplingFactor" />
      <MemberType>Property</MemberType>
