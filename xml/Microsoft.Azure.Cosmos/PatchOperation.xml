      <MemberSignature Language="C#" Value="public static Microsoft.Azure.Cosmos.PatchOperation Replace&lt;T&gt; (string path, T value);" />
      <MemberSignature Language="ILAsm" Value=".method public static hidebysig class Microsoft.Azure.Cosmos.PatchOperation Replace&lt;T&gt;(string path, !!T value) cil managed" />
