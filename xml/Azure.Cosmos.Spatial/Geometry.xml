    <Member MemberName="Within">
      <MemberSignature Language="C#" Value="public bool Within (Azure.Cosmos.Spatial.Geometry outer);" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig instance bool Within(class Azure.Cosmos.Spatial.Geometry outer) cil managed" />
      <MemberSignature Language="DocId" Value="M:Azure.Cosmos.Spatial.Geometry.Within(Azure.Cosmos.Spatial.Geometry)" />
      <MemberSignature Language="VB.NET" Value="Public Function Within (outer As Geometry) As Boolean" />
