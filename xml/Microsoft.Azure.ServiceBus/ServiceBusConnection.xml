    <Member MemberName="ConnectionIdleTimeout">
      <MemberSignature Language="C#" Value="public TimeSpan? ConnectionIdleTimeout { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.Nullable`1&lt;valuetype System.TimeSpan&gt; ConnectionIdleTimeout" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Azure.ServiceBus.ServiceBusConnection.ConnectionIdleTimeout" />
