      "version": {
        "api/overview/azure/**/*.yml": ["azure-dotnet"],
        "api/overview/azure/**/*.md": ["azure-dotnet"],
        "docs-ref-conceptual/*.md": ["azure-dotnet"]
      },
      "monikers": {
        "api/overview/azure/**/*.yml": ["azure-dotnet"],
        "api/overview/azure/**/*.md": ["azure-dotnet"],
        "docs-ref-conceptual/*.md": ["azure-dotnet"]
      },
      "langs": {
        "api/**.yml": ["csharp", "vb", "fsharp"]
      },
      "ms.topic": {
        "api/**.yml": "managed-reference"
      },
      "ms.custom": {
        "api/overview/azure/**/*.md": ["azure-dotnet-devcenter","azure-dotnet-devcenter-authored","azure-dotnet-svc-overview"],
        "docs-ref-conceptual/**/*.md": ["azure-dotnet-devcenter","azure-dotnet-devcenter-authored","azure-dotnet-devcenter-conceptual"],
        "docs-ref-conceptual/**/*.yml": ["azure-dotnet-devcenter","azure-dotnet-devcenter-authored","azure-dotnet-devcenter-conceptual"],
        "api/**.yml": ["azure-dotnet-devcenter","azure-dotnet-devcenter-generated"]        
      }
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "template": [
      "docs.html"
    ],
    "lruSize": 0,
    "debug": true
  }
}
