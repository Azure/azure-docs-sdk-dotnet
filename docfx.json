  "build": {
    "content": [
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "exclude": [
          "**/toc.yml",
          "overwrites/**/*",
          "**/includes/**"
        ],
        "src": "api",
        "dest": "api"
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": "api",
        "dest": "api/azure_ref_toc"
