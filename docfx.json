{
  "build": {
    "content": [
      {
        "files": [
          "*.yml"
        ],
        "src": "docs-ref-from-xml",
        "dest": "."
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "docs-ref-from-xml/_splitted/"
      },
      {
        "files": [
          "index.md"
        ]
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "docs-ref-conceptual",
        "dest": "docs-ref-conceptual"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs-ref-overwrite/**/*.md"
        ]
      }
    ],
    "globalMetadata": {
      "author": "erickson-doug",
      "ms.author": "douge",
      "manager": "douge",
      "ms.topic": "reference",
      "ms.devlang": "csharp",
      "ms.date": "11/16/2016",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "brand": "azure",
      "apiPlatform": "dotnet",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "folder_relative_path_in_docset": "api-index"
        },
        "breadcrumb/": {
          "folder_relative_path_in_docset": "breadcrumb"
        },
        "docs-ref-autogen/": {
          "folder_relative_path_in_docset": "docs-ref-autogen"
        },
        "docs-ref-manual/": {
          "folder_relative_path_in_docset": "docs-ref-manual"
        },
        "docs-ref-overwrite/": {
          "folder_relative_path_in_docset": "docs-ref-overwrite"
        },
        "scripts/": {
          "folder_relative_path_in_docset": "scripts"
        }
      }
    },
    "fileMetadata": {
      "ms.service": {
        "docs-ref-autogen/ApplicationInsights/**.yml": "application-insights",
        "docs-ref-autogen/Automation/**.yml": "automation",
        "docs-ref-autogen/AzureResourceManager/**.yml": "azure-rosource-manager",
        "docs-ref-autogen/Backup/**.yml": "backup",
        "docs-ref-autogen/Batch/**.yml": "batch",
        "docs-ref-autogen/Billing/**.yml": "Billing",
        "docs-ref-autogen/cdn/**.yml": "cdn",
        "docs-ref-autogen/dns/**.yml": "dns",
        "docs-ref-autogen/Compute/**.yml": "cloud-services",
        "docs-ref-autogen/DataFactory/**.yml": "data-factory",
        "docs-ref-autogen/DataLakeAnalytics/**.yml": "data-lake-analytics",
        "docs-ref-autogen/DataLakeStore/**.yml": "data-lake-store",
        "docs-ref-autogen/Data/EventHub/**.yml": "event-hubs",
        "docs-ref-autogen/EventHub/**.yml": "event-hubs",
        "docs-ref-autogen/IotHub/**.yml": "iot-hub",
        "docs-ref-autogen/Keyvault/**.yml": "key-vault",
        "docs-ref-autogen/Media/**.yml": "media-services",
        "docs-ref-autogen/Network/**.yml": "vitual-network",
        "docs-ref-autogen/NotificationHubs/**.yml": "notification-hubs",
        "docs-ref-autogen/PowerBIEmbedded/**.yml": "powerbi",
        "docs-ref-autogen/RedisCache/**.yml": "redis-cache",
        "docs-ref-autogen/Search/**.yml": "search",
        "docs-ref-autogen/ServiceBus/**.yml": "service-bus",
        "docs-ref-autogen/Sql/**.yml": "sql-database",
