        "api/Microsoft.Azure.ServiceBus*.yml": "service-bus",
        "api/Microsoft.ServiceBus*.yml": "service-bus",
        "api/Microsoft.Azure.Management.Fluent*.yml": "service-bus",
        "api/Microsoft.ServiceFabric*.yml": "service-fabric",
        "api/System.Fabric*.yml": "service-fabric",
        "api/Microsoft.Xml.Serialization.GeneratedAssembly*.yml": "service-fabric",
        "api/Microsoft.Azure.Management.Sql*.yml": "sql-database",
        "api/Microsoft.Azure.SqlDatabase.ElasticScale*.yml": "sql-databse",
        "api/Microsoft.Azure.Management.Storage*.yml": "storage",
        "api/Microsoft.WindowsAzure.Storage*.yml": "storage",
        "api/Microsoft.Azure.Management.StreamAnalytics*.yml": "stream-analytics",
        "api/Microsoft.Azure.Management.TrafficManager*.yml": "traffic-manager",
        "api/Microsoft.Azure.Management.Compute*.yml": "big-compute",
