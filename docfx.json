        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/includes/**"
        ],
        "src": "docs-ref-conceptual",
        "dest": "azure"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
