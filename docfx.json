        "api/Microsoft.Azure.Management.Graph.RBAC.Fluent*.yml": "cosmos db"
        
      },
      "version": {
        "api/overview/azure/**/*.yml": ["azure-dotnet"],
        "api/overview/azure/**/*.md": ["azure-dotnet"],
        "docs-ref-conceptual/*.md": ["azure-dotnet"]
      },
      "monikers": {
        "api/overview/azure/**/*.yml": ["azure-dotnet"],
        "api/overview/azure/**/*.md": ["azure-dotnet"],
        "docs-ref-conceptual/*.md": ["azure-dotnet"]
      },
      "langs": {
        "api/**.yml": ["csharp", "vb", "fsharp","cppcx","cppwinrt","cpp"]
      }
    },
    "xref": [
      "msdn.4.5.2.zip"
    ],
    "dest": "_site",
    "template": [
      "docs.html"
    ],
    "lruSize": 0,
    "debug": true
  }
}
