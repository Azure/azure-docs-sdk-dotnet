        "docs-ref-autogen/StreamAnalytics/**.yml": "stream-analytics",
        "docs-ref-autogen/elastic-db-tools/**.yml": "",
        "docs-ref-autogen/websites/**.yml": "websites",
        "docs-ref-autogen/Data/Batch/**.yml": "batch",
        "docs-ref-autogen/Data/Iot/**.yml": "iot-hub",
        "docs-ref-autogen/Data/KeyVault/**.yml": "key-vault",
        "docs-ref-autogen/Data/MediaServices/**.yml": "media-services",
        "docs-ref-autogen/Data/MediaServicesExtensions/**.yml": "media-services",
        "docs-ref-autogen/Data/PowerBI/**.yml": "powerbi",
        "docs-ref-autogen/Data/Search/**.yml": "search",
        "docs-ref-manual/Data/ServiceBus-Messaging/**.yml": "service-bus-messaging",
